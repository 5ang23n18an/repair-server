<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wqtang.SystemConfigMapper">

    <select id="selectByKey" resultType="com.wqtang.object.po.system.SystemConfig">
        select *
        from system_config
        <where>
            `key` = #{key}
        </where>
    </select>

</mapper>